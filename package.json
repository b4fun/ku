{
  "name": "ku",
  "version": "0.0.1",
  "workspaces": [
    "ui/",
    "browser-ui/",
    "prql-browser",
    "kql/",
    "protos/",
    "prql-js"
  ],
  "scripts": {
    "build:base": "npm run build -w protos -w kql -w prql-js -w ui",
